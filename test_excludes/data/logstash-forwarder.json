{
  "network": {
    "servers": [ "localhost:3333" ],
    "ssl ca": "/etc/logstash-forwarder/ssl/server.crt"
  },
  "files": [
    {
      "paths": [ 
        "/var/log/test*.log"
      ],
      "exclude": [
        "/var/log/test1.log",
        "/var/log/test3.log"
      ],
      "fields": { 
        "type": "test-files" 
      }
    },
    {
      "paths": [ 
        "/var/log/some-other-file1.log"
      ],
      "fields": { 
        "type": "other" 
      }
    },
    {
      "paths": [ 
        "/var/log/syslog"
      ],
      "fields": { 
        "type": "syslog" 
      }
    }
  ]
}
